package PrakPBO;

class Hewan2 {
    private String nama;
    
    public Hewan2(String nama) {
        this.nama = nama;
    }
    
    public String getNama() {
        return nama;
    }
    
    public void display() {
        System.out.println("Nama hewan: " + nama);
    }
}

class Kucing extends Hewan2 {
    public Kucing(String nama) {
        super(nama);
    }
    
    public void getInfo() {
        System.out.println("Ini adalah kucing bernama " + getNama());
    }
}

public class Soal4 {
    public static void main(String[] args) {
        Kucing k = new Kucing("Mimi");
        k.display();
        k.getInfo();
    }
}
