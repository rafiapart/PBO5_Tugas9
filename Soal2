package PrakPBO;

class Tingkat {
    protected int panjang;
    protected int lebar;
    protected int tinggi;
    
    public int hitungLuas() {
        return panjang * lebar;
    }
}

public class Soal2 {
    public static void main(String[] args) {
        Tingkat t = new Tingkat();
        t.panjang = 10;
        t.lebar = 5;
        System.out.println("Luas: " + t.hitungLuas());
    }
}
